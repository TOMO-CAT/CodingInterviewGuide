{
    "cSpell.words": [
        "cout",
        "deque",
        "endl",
        "iostream",
        "nullptr",
        "nums",
        "struct"
    ],
    // 屏蔽 markdownlint 的一些规则
    "markdownlint.config": {
        "MD040": false,
        "MD045": false,
        "MD024": false,
    },
    // format on save
    "editor.formatOnSave": true,
    // 在文件末尾加入空行
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
}
